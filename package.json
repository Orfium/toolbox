    "build:partial:types": "tsc -p tsconfig.json",
    "build": "rollup -c",
    "build:development": "yarn build && yalc push",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "documentation:generate": "typedoc --out typeDocs src",
    "documentation:up": "node server/docs-server.js"
  },
  "keywords": [
    "Orfium",
    "typescript",
    "react",
    "tooling",
    "tools",
    "toolbox"
