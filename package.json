{
  "name": "@orfium/toolbox",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.26.1",
    "axios-mock-adapter": "^1.20.0",
    "react-query": "^3.34.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "prepare": "yarn build && husky install",
    "documentation": "cd documentation && yarn start",
    "prebuild": "rimraf dist",
    "build:partial:default": "cross-env NODE_ENV=production BABEL_ENV=production babel src --config-file ./babelrc.build.json --extensions '.ts,.tsx' --out-dir ./dist",
    "build:partial:types": "tsc --emitDeclarationOnly",
    "build": "yarn build:partial:default && yarn build:partial:types",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "documentation:generate": "typedoc --out typeDocs src",
    "documentation:up": "node server/docs-server.js"
  },
  "keywords": [
    "Orfium",
    "typescript",
    "react",
    "tooling",
    "tools",
    "toolbox"
  ]
}
